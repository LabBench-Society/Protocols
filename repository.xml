<?xml version="1.0" encoding="utf-8" ?>
<repository xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="https://labbench.io/xsd/5.0/repository.xsd"
            id="labbench.io"
            name="LabBench Protocol Repository">

    <category name="Getting Started" id="intro">
        <protocol id="intro.labbench"
            name="Introduction to LabBench"
            labbench-version="5.0"
            category="Psychometrics;Demonstration Protocol"/>
        <protocol id="intro.thrEstimation"
            name="Introduction to Estimation of Thresholds and Psychometric Functions"
            labbench-version="5.0"
            category="Thresholds;Psychopmetric Functions;Demonstration Protocol"/>
        <protocol id="intro.randomization"
            name="Introduction to Randomization and Templating"
            labbench-version="5.0"
            category="Randomization;Templating;Demonstration Protocol"/>
        <protocol id="intro.cpar"
            name="Introduction to Cuff Pressure Algometry"
            labbench-version="5.0"
            category="Pressure Algometry;Demonstration Protocol"/>
        <protocol id="intro.imageGeneration"
            name="Introduction to Image Generation"
            labbench-version="5.0"
            category="Demonstration Protocol"/>
        <protocol id="intro.ratings"
            name="Introduction to Psychophysical Rating Scales"
            labbench-version="5.0"
            category="Demonstration Protocol"/>
    </category>

    <category name="Cuff Pressure Algometry" id="cpa">
        <protocol id="cpa.tscpm2ap"
            name="Temporal Summation and Conditioned Pain Modulation Protocol (2AP)" 
            labbench-version="5.0"
            category="Pressure Algometry;Temporal Summation;Conditioned Pain Modulation"/>
        <protocol id="cpa.tscpm3ap"
            name="Temporal Summation and Conditioned Pain Modulation Protocol (3AP)" 
            labbench-version="5.0"
            category="Pressure Algometry;Temporal Summation;Conditioned Pain Modulation"/>
        <protocol id="cpa.tscpm.validation"
            name="Temporal Summation and Conditioned Pain Modulation Protocol (Compressor Validation)" 
            labbench-version="5.0"
            category="Pressure Algometry;Validation Protocol;LabBench CPAR+"/>            
        <!-- 
        <protocol id="cpa.stscpm3ap"
            name="Spatial, Temporal Summation and Conditioned Pain Modulation Protocol (3AP)"
            labbench-version="5.0"
            category="Pressure Algometry;Spatial Summation;Temporal Summation;Conditioned Pain Modulation"/>       
        --> 
    </category> 

    <category name="Cognitive Tasks" id="cogniCategory">
        <category name="Stop Signal Tasks" id="cogni.sst">
            <protocol id="cogni.sst.task"
                name="Stop Signal Task"
                labbench-version="5.0"
                category="Cognitive Task"/>      
            <protocol id="cogni.sst.game"
                name="Stop Signal Game"
                labbench-version="5.0"
                category="Cognitive Task"/>              
        </category>
        <category name="Stroop Tasks" id="cogni.stroop">
            <protocol id="cogni.stroop.task"
                name="Stroop Task"
                labbench-version="5.0"
                category="Cognitive Task"/>
            <protocol id="cogni.stroop.reverse"
                name="Reverse Stroop Task"
                labbench-version="5.0"
                category="Cognitive Task"/>
        </category>
        <category name="Flanker Tasks" id="cogni.flanker">
            <protocol id="cogni.flanker.task"
                name="Flanker Task"
                labbench-version="5.0"
                category="Cognitive Task"/>
        </category>
    </category>

    <category name="Electroencephalography" id="eegCategory">
        <protocol id="stopsignalEEG"
            name="Stop Signal Task (Electroencephalography)"
            labbench-version="5.0"
            category="Cognitive Task;EEG"/>      
        <protocol id="stroopEEG"
            name="Stroop Task (Electroencephalography)"
            labbench-version="5.0"
            category="Cognitive Task;EEG"/>
        <protocol id="stroopReverseEEG"
            name="Reverse Stroop Task (Electroencephalography)"
            labbench-version="5.0"
            category="Cognitive Task;EEG"/>
        <protocol id="aoddball"
            name="Oddball Task (Auditory)"
            labbench-version="5.0"
            category="EEG" />
        <protocol id="voddball"
            name="Oddball Task (Visual)"
            labbench-version="5.0"
            category="EEG" />    
    </category>

    <category name="Itch Assessment" id="itchCategory">
        <protocol id="hyperknesisDemo"
            name="Hyperknesis"
            labbench-version="5.0"
            category="Psychometrics;Demonstration Protocol"/>
        <protocol id="alloknesisDemo"
            name="Alloknesis"
            labbench-version="5.0"
            category="Psychometrics;Demonstration Protocol"/>
    </category>

    <category name="Calibration" id="Calibration">
        <protocol id="scCalibration"
            name="Screen and Fiducial Calibration Protocol"
            labbench-version="5.0"
            category="Visual Stimuli;Validation Protocol"/>
    </category>
</repository>
